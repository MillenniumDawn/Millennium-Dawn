	state = { name = "idle"				animation = "long_idle05"		animation_blend_time = 0.3 animation_speed = 1.0 chance = 1 looping = no }
	state = { name = "training"				animation = "training" 			animation_blend_time = 0.3 animation_speed = 1.0 chance = 2 looping = no }
	state = { name = "training"				animation = "jumping_jacks" 	animation_blend_time = 0.3 animation_speed = 1.0 chance = 1 looping = no }
	state = { name = "training"				animation = "pushup" 			animation_blend_time = 0.3 animation_speed = 1.0 chance = 1 looping = no }
	state = { name = "training"				animation = "aim_exercise" 		animation_blend_time = 0.3 animation_speed = 1.0 chance = 1 looping = no }

	attach = { 	name = "rifle1"	Right_Hand_node = "ITA_weapon_AR70_right_entity" }
	attach = { 	name = "rifle4"	Root_node_2 = "ITA_weapon_AR70_right_entity" }
	attach = { 	name = "rifle2"	Left_Hand_node = "ITA_weapon_AR70_left_entity" }
	attach = { 	name = "rifle3"	mid_back_node = "ITA_weapon_AR70_long_idle_entity" }

	attach = { 	name = "lighter"			Right_Hand_node_4 = "lighter_entity" }
