		containerWindowType = {
			name = "UAR_stats_box" # Haha pink shirt
			position = { x = 35 y = 130 }
			size = { width = 350 height = 430 }
			# slider on the right hand side
			verticalScrollbar = "left_vertical_slider"
			# margin just does whatever the fuck it wants
			margin = { top = 8 bottom = 8 } # TODO: Temp variable until we can change this until we need

			# This is needed to give the stats box a "Body" without it you can't find it if it decides to do a houdini
			background = {
				name = "Background"
				quadTextureSprite ="GFX_tiled_window_transparent"
			}

			# gridboxType name must match the dynamic_list name in the scripted_gui.txt
			# position is obivous and padding creates a pixel buffer between the entry and the next or the previous. Depends on which number you are. +50 pixels (me) +50 pixels
			# size makes it the same size as the stats box if you are using %%. This makes it so if you scale up or down the stats box it scales with it.
			# slotsize. This is the actual size of the entry itself. So it can only be 80 pixels. You'll have trail off if you have like a stupid ass name or if you have something that is too long
			# format. You're not retard. I don't need to explain what this is
			# max_slots_horizontal allows you to do entries side by side if you do 1+. i.e.
			# value of 2:
			# one_name two_name
			# value of 1:
			# one_name
			# two_name
			gridboxType = {
				name = "UAR_dynamic_tag_list"
				position = { x = 50 y = 0 }
				padding = { top = 50 bottom = 50 }
				size = { width = 100%% height = 100%% }
				slotsize = { width = 80 height = 50 }
				format = "UPPER_LEFT"
				max_slots_horizontal = 1
			}
		}
